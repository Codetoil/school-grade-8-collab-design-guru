<!DOCTYPE xml>
<!--I use this for multilined values!-->
<root xmlns:lit="https://github.com/Littoil/school-grade-8-collab-design-guru">
	<lit:root_slide_group>
		<lit:slide id="home">
				<lit:data>Anthony's&lt;br&gt;Presentation</lit:data>
				<lit:buttons>
					<lit:button>
						<lit:name>Topic 1</lit:name>
						<lit:goto>topic 1</lit:goto></lit:button>
					<lit:button>
						<lit:name>Topic 2</lit:name>
						<lit:goto>topic 2</lit:goto></lit:button></lit:buttons></lit:slide>
		<lit:slide_group id="topic 1">
			<lit:data></lit:data>
			<lit:slide id="buttonActions">
				<lit:data></lit:data>
				<lit:buttons>
					<lit:button>
						<lit:name>go back to topic 1 screen</lit:name>
						<lit:goto>topic1</lit:goto></lit:button>
					<lit:button>
						<lit:name>go back to home screen</lit:name>
						<lit:goto>home</lit:goto></lit:button></lit:buttons></lit:slide>
			<lit:buttons>
				<lit:button>
					<lit:name>Home</lit:name>
					<lit:goto>home</lit:goto></lit:button>
				<lit:button>
					<lit:name>button actions</lit:name>
					<lit:goto>buttonAction</lit:goto></lit:button>
				<lit:button>
					<lit:name>go to topic 2</lit:name>
					<lit:goto>topic2</lit:goto></lit:button></lit:buttons></lit:slide_group>
		<lit:slide_group id="topic 2">
			<lit:data></lit:data>
			<lit:slide id="settingButtonColors">
				<lit:data></lit:data>
				<lit:buttons>
					<lit:button>
						<lit:name>Home</lit:name>
						<lit:goto>home</lit:goto></lit:button>
					<lit:button>
						<lit:name>Back to button Actions</lit:name>
						<lit:goto>buttonAction</lit:goto></lit:button>
					<lit:button>
						<lit:name>Back to Topic 2</lit:name>
						<lit:goto>topic 2</lit:goto></lit:button></lit:buttons></lit:slide>
			<lit:buttons>
				<lit:button>
					<lit:name>setting a button color</lit:name>
					<lit:goto>buttonColor</lit:goto></lit:button>
				<lit:button>
					<lit:name>Home</lit:name>
					<lit:goto>home</lit:goto></lit:button></lit:buttons></lit:slide_group></lit:root_slide_group>
</root>
